<nodes_config>
    <node 
          name="input"
          class="InputService"
          transport_name="fs"
          type="both"
    />
    <node 
          name="extract"
          class="ExtractService"
          type="both"
          conf_path="conf/fe"
          parent_list_name="preprocess"
          feature_list_name="context"
          item_message_path="app_distribute_item_feature.item_feature[*].app_id"
          fs_node_name="input"
          max_num_each_slice="10"
    />
    <node 
          name="embedding_local"
          class="EmbeddingServiceLocal"
          fe_node_name="extract"
          model_name="aaaa"
    />
    <node 
          name="concat"
          class="ConcatService"
          fe_name="extract"
          emb_name="embedding_local"
          model_name="aaaa"
          max_num_each_slice="10"
    />
    <node 
          name="model"
          class="ModelService"
          model_name="aaaa"
          concat_node_name="concat"
          fe_node_name="extract"
          model_type="ctr"
    />
    <node 
          name="response"
          class="ResponseService"
          model_node_name="model"
    />
    <node 
          name="dump_log"
          class="DumpLogService"
    />
    <node
          name="kafka_log"
          class="DumpLogKafkaService"
          to_file_rate="1"
          target_path="./debug_data"
          limited_num="100"
          model_name="model"
    />
    <node
          name="kafka_log_file"
          class="DumpLogKafkaService"
          to_file_rate="0.001"
          target_path="./debug_data"
          limited_num="100"
          model_name="model"
    />
    <node
          name="debug_data"
          class="DebugDataService"
          data_dir_path="./debug_data"
          limited_data_num="100"
    />
    <node
          name="replace_req"
          class="ReplaceReqService"
    />
    <node
          name="local_consis"
          class="LocalConsistencyService"
    />
    <node
          name="fake_fs_rsp"
          class="FakeFSRspService"
          fs_node_name="input"
    />

</nodes_config>

